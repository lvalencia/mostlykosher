<div class="row" id="new" data-parallax="base">
  <div class="new">
    <%= render partial: 'client/landing/separator', locals: { color:'#f36b10', text: I18n.t(:header, scope: [:client, :news]) } %>
  </div>
  <div class="new-content">
    <div class="main">
      <p><%= I18n.t(:title, scope: [:client, :news]) %></p><br/>
      <%= render partial: 'shared/review', collection: @reviews, as: :review %>
      <p class="press">
        <%= link_to I18n.t(:press_kit_link, scope: [:client, :news]), '/press-kit.pdf' %>
      </p>
      <br/>
    </div>
  </div>
</div>
